{
  "extends": ["airbnb-base", "plugin:@typescript-eslint/recommended", "prettier"],
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["import", "@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-explicit-any": 2,
    "no-console": 0,
    "import/extensions": 0,
    "no-shadow": "off",
    "@typescript-eslint/no-shadow": "warn"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ts"]
      }
    }
  }
}